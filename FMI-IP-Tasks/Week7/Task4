#include <iostream>

using namespace std;

const int MAX_SIZE = 100;

int lenght(char arr[])
{
    int i;
    int counter;
    
    while(arr[i] != '\0')
    {
        counter++;
        i++;
    }
    return counter;
}

int main()
{
    char arr[MAX_SIZE];
    cin.getline[];
    lenght(arr);
    return 0;
}
