#include <iostream>
#include <cmath>

using namespace std;

bool isPrime(int number)
{
    
    for(int j = 2; j <= sqrt(number); j++)
    {
        if(number % j == 0)
        {
            return false;
        }
    }
    return true;
}

int primeNumbersLessThanHundred()
{
    for(int i = 2; i <= 100; i++)
    {
        if(isPrime(i))
        {
            return i;
        }
    }
    return 0;
}

void printPrime()
{
    for(int i = 2; i <= 100; i++)
    {
        if(isPrime(i))
        {
            cout << i << " ";
        }
    }
}

int main()
{
    printPrime();
}
