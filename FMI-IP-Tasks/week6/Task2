#include <iostream>
#include <cmath>

using namespace std;

bool isPrime(int n)
{
    if(n == 2)
    {
      return true;
    }
    if(n == 0 || n == 1 || n % 2 == 0)
    {
        return false;
    }
    
    for(int i = 3; i <= sqrt(n); i+= 2)
    {
        if(n % i == 0)
        {
            return false;
        }
    }
    return true;
}

int primeLessThanOneHundred()
{
    for(int i = 2; i <= 100; i++)
    {
        if(isPrime(true))
        {
            return i;
        }
    }
}

void printPrime()
{
    for(int i = 2; i <= 100; i++)
    {
        if(isPrime(true))
        {
            cout << i << endl;
        }
    }
}

int main()
{
    printPrime();
    return 0;
}
