#include <iostream>
#include <cmath>

using namespace std;

bool isPrime(int number)
{
    
    for(int i = 2; i <= sqrt(number); i ++)
    {
        if(number % i == 0)
        {
            return false;
        }
    }
    
    return true;
}

int reversedNumber(int number)
{
    int digit = 0;
    int numberKeeper = 0;
    
    while(number != 0)
    {
        digit = number % 10;
        numberKeeper = (numberKeeper * 10) + digit;
        number /= 10;
    }
}

bool isPalindrome(int number, int numberKeeper)
{
    if(number == numberKeeper)
    {
        return true;
    }
}

void printsNumbersThatArePrimeAndPalindrome(unsigned int n)
{
    int counter = 2;
    while(n >= 0)
    {
        if(isPrime(counter) && isPalindrome(counter))
        {
            cout << counter << " ";
            counter ++;
            n--;
        }
        counter++;
    }
}

int main()
{
    printsNumbersThatArePrimeAndPalindrome();
    return 0;
}
